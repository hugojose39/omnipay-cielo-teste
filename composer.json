{
    "name": "hugojose39/omnipay-cielo-teste",
    "description": "Cielo driver for the Omnipay payment processing library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\CieloTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\CieloTest\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Hugo Jose",
            "email": "hugojose39@yahoo.com"
        }
    ],
    "require": {
        "omnipay/common": "^3.0"
    },  
    "require-dev": {
        "omnipay/tests": "^4.1"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
