{
    "name": "hugojose39/omnipay-cielo-teste",
    "description": "Cielo driver for the Omnipay payment processing library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hugojose39\\OmnipayCieloTeste\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hugojose39\\OmnipayCieloTeste\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Hugo Jose",
            "email": "hugojose39@yahoo.com"
        }
    ],
    "require": {
        "omnipay/common": "^3.0",
        "symfony/http-client": "^7.0",
        "php-http/httplug": "^2.4",
        "nyholm/psr7": "^1.8"
    },    
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "omnipay/tests": "^4.1"
    }
}
